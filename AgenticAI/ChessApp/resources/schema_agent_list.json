{
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": {
                "type": "integer",
                "description": "Unique identifier"
            },
            "type": {
                "type": "string",
                "enum": ["REMOTE_AGENT", "LOCAL_AGENT", "LOCAL_USER", "REMOTE_USER"],
                "description": "Type of agent - either REMOTE_AGENT or USER"
            },
            "name": {
                "type": "string",
                "description": "Display name"
            },
            "description": {
                "type": "string",
                "description": "Brief description"
            },
            "model": {
                "type": "string",
                "description": "Model identifier for remote AI agents"
            },
            "api_url": {
                "type": "string",
                "format": "uri",
                "description": "API endpoint URL"
            },
            "api_key": {
                "type": "string",
                "description": "API key for authentication"
            },
            "enabled": {
                "type": "boolean",
                "description": "Working state"
            }
        },
        "required": ["id", "type", "name", "description", "model", "api_url", "api_key", "enabled"],
        "additionalProperties": false
    }
}